{
  "name": "drifter",
  "version": "0.1.0",
  "description": "use your browser's poor timing to make rich musical gems!",
  "scripts": {
    "start:pretotype": "npx serve pretotype",
    "deploy": "gh-pages -b gh-deploy -d pretotype",
    "deploy:off": "gh-pages -b gh-deploy -d build",
    "start": "env PUBLIC_URL='.' react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliranmal/drifter.git"
  },
  "author": "eliran malka",
  "bugs": {
    "url": "https://github.com/eliranmal/drifter/issues"
  },
  "homepage": "https://github.com/eliranmal/drifter#readme",
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
